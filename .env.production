# Production Environment Variables for kanpAI Docker Deployment
# CRITICAL: Replace ALL values with actual production keys before deployment!

# Database Configuration
DB_PASSWORD=REPLACE_WITH_STRONG_DATABASE_PASSWORD_HERE
REDIS_PASSWORD=REPLACE_WITH_STRONG_REDIS_PASSWORD_HERE

# Security (Critical)
JWT_SECRET=REPLACE_WITH_STRONG_64_CHAR_RANDOM_STRING_EXAMPLE_abcd1234xyz

# External APIs (Required)
OPENAI_API_KEY=sk-proj-YOUR_PRODUCTION_OPENAI_KEY_HERE
LINE_CHANNEL_ACCESS_TOKEN=YOUR_PRODUCTION_LINE_CHANNEL_TOKEN
LINE_CHANNEL_SECRET=YOUR_PRODUCTION_LINE_CHANNEL_SECRET
STRIPE_API_KEY=sk_live_YOUR_PRODUCTION_STRIPE_SECRET_KEY
STRIPE_WEBHOOK_SECRET=whsec_YOUR_PRODUCTION_WEBHOOK_SECRET
STRIPE_PUBLISHABLE_KEY=pk_live_YOUR_PRODUCTION_STRIPE_PUBLISHABLE_KEY

# URLs (Replace with your actual domain)
FRONTEND_URL=https://kanpai-plus.jp
ADMIN_URL=https://admin.kanpai-plus.jp
API_BASE_URL=https://api.kanpai-plus.jp

# Google Calendar Integration
GOOGLE_CALENDAR_ID=your_production_calendar_id@gmail.com
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRODUCTION_PRIVATE_KEY\n-----END PRIVATE KEY-----"